def bulk_insert_configurations(data, cnsmr_ingestion_id):
    # Create a list of model instances
    configurations = [
        DqaasApplConfigModel(
            env=config['env'],
            key_nm=config['key_nm'],
            val_tx=config['val_tx'],
            cnsmr_ingestion_id=cnsmr_ingestion_id
        )
        for config in data
    ]
    
    # Bulk insert
    db.session.add_all(configurations)
    db.session.commit()
