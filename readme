def contract_expiry_email_content(
    contract: dict, contract_due_date, app_name, dcaas_web_url, product_name
):
    with open(TEMPLATE_FOLDER/"ContractTableContent.html", "r") as fp:
        contract_table_content = fp.read()
    contract_table_content = render_template_string(contract_table_content, contract=contract)
    context = {
        "contract_due_date": contract_due_date,
        "app_name": app_name,
        "dcaas_web_url": dcaas_web_url,
        "product_name": product_name,
        "contract_table_content": contract_table_content
    }
    with open(TEMPLATE_FOLDER/"ContractExpiryContent.html", "r") as fp:
        contract_expiry_content = fp.read()
    contract_expiry_content = render_template_string(contract_expiry_content, **context)

    return contract_expiry_content
