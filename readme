import extract_msg

def extract_html_from_msg(file_path):
    with extract_msg.openMsg(file_path) as msg:
        if msg.htmlBody:
            return msg.htmlBody
        else:
            return None

# Example usage
msg_file_path = 'path_to_your_file.msg'
html_content = extract_html_from_msg(msg_file_path)
print(html_content)
