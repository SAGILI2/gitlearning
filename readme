class ProfileDetailsMasterSchema(ma.SQLAlchemyAutoSchema):
    class Meta:
        model = ProfileDetailsMasterModel
        include_fk = True
        load_instance = True
        transient = True

    appl_nm = ma.Method("get_appl_nm")
    appl_id = ma.Method("get_appl_id")

    def get_appl_nm(self, obj):
        return obj.dqaas_cnsmr_appl_info.appl_nm if obj.dqaas_cnsmr_appl_info else None

    def get_appl_id(self, obj):
        return obj.dqaas_cnsmr_appl_info.appl_id if obj.dqaas_cnsmr_appl_info else None
