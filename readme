    @classmethod
    def get_active_rules(cls):
        return cls.query.join(
            RuleDtl, cls.rule_id == RuleDtl.rule_id
        ).filter(
            cls.rule_id > 1,
            RuleDtl.rule_actv_in == 'Y'
        ).with_entities(
            cls.dstr_id_tx,
            cls.rule_id
        ).distinct().all()
